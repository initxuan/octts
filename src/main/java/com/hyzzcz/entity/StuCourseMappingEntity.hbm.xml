<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hyzzcz.entity.StuCourseMappingEntity" table="stu_course_mapping" schema="OCTTS">
        <property name="id">
            <column name="id" sql-type="int(11) unsigned"/>
        </property>
        <property name="stuId">
            <column name="stu_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="courseId">
            <column name="course_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="grade">
            <column name="grade" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="sort">
            <column name="sort" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>