<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.TeamEntity" table="team" schema="OCTTS">
        <property name="id">
            <column name="id" sql-type="int(11) unsigned"/>
        </property>
        <property name="teamNum">
            <column name="team_num" sql-type="int(20)" not-null="true"/>
        </property>
        <property name="leaderId">
            <column name="leader_id" sql-type="int(20)" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(11)" length="11" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>